@{ 
    var currentCommit = AdminLte.Properties.Resources.CurrentCommit;
    var info = "";
    if(currentCommit != null)
    {
        var infos = currentCommit.Split("\n");
        info = infos.Length > 0 ? infos[0] : "-";

    }
}

<footer class="main-footer">
    <!-- To the right -->
    <div class="float-right d-none d-sm-inline">
        @info
    </div>
